<section id="timeline">
  <div id="graphic-hover"></div>
  <div id="graphic-main-container">
    <div id="graphic-main-0" class="main-text"></div>
    <div id="graphic-main-1" class="main-text"></div>
    <div id="graphic-main-2" class="main-text"></div>
    <div id="graphic-main-3" class="main-text"></div>
  </div>
  <div id="graphic-wrapper">
    <svg
      id="graphic-container"
      width="100%"
      height="100%"
      preserveAspectRatio="xMidYMid meet"
    >
      <g id="base-group"></g>
      <path id="line-0"></path>
      <path id="line-1"></path>
      <path id="line-2"></path>
      <path id="line-3"></path>
      {{#each copy.timeline}}
        <circle
          data-date="{{{this.date}}}"
          data-type="{{{this.type}}}"
          data-shape="{{{this.shape}}}"
          data-hover="{{{this.hover}}}"
          data-main="{{{this.main}}}"
          data-influence="{{{this.influence}}}"
        ></circle>
      {{/each}}
    </svg>
  </div>
</section>
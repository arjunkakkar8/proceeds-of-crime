<section id="timeline">
  <div id="graphic-wrapper">
    <svg
      id="graphic-container"
      width="100%"
      height="100%"
      preserveAspectRatio="xMidYMid meet"
    >
      <g id="base-group"></g>
      {{#each copy.timeline}}
        <circle
          data-date="{{{this.date}}}"
          data-type="{{{this.type}}}"
          data-shape="{{{this.shape}}}"
          data-hover="{{{this.hover}}}"
          data-main="{{{this.main}}}"
          data-influence="{{{this.influence}}}"
        ></circle>
      {{/each}}
    </svg>
  </div>
  <div id="graphic-hover"></div>
  <div id="graphic-main-a" class="main-text"></div>
  <div id="graphic-main-b" class="main-text"></div>
  <div id="graphic-main-c" class="main-text"></div>
  <div id="graphic-main-d" class="main-text"></div>
</section>